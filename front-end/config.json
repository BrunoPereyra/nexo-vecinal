{
    "expo": {
        "name": "front-end",
        "slug": "front-end",
        "version": "1.3.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo-nexovecinal.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "privacyPolicy": "https://nexo-vecinal.blogspot.com/2025/03/terminos-y-condiciones-y-politica-de.html",
        "android": {
            "package": "com.nexovecinal.app",
            "versionCode": 40,
            "permissions": [
                "ACCESS_FINE_LOCATION",
                "ACCESS_COARSE_LOCATION"
            ],
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo-nexovecinal-transparente.png",
                "backgroundColor": "#EAE6DA"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/logo-nexovecinal.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/logo-nexovecinal-transparente.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#EAE6DA"
                }
            ],
            [
                "expo-build-properties",
                {
                    "android": {
                        "removePermissions": []
                    }
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "eas": {
                "projectId": "986130da-6534-4144-94be-e74db5d65c82"
            },
            "EXPO_URL_API": "https://www.pinkker.tv/9000",
            "EXPO_URL_APIWS": "wss://www.pinkker.tv/9000",
            "router": {
                "origin": false
            }
        }
    }
}