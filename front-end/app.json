{
  "expo": {
    "name": "nexo vecinal",
    "slug": "nexo-vecinal",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "notification": {
      "icon": "./assets/images/adaptive-icon.png",
      "color": "#000000",
      "iosDisplayInForeground": true
    },
    "privacyPolicy": "https://nexo-vecinal.blogspot.com/2025/03/terminos-y-condiciones-y-politica-de.html",
    "android": {
      "package": "com.nexovecinal.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 8,
      "config": {
        "googleMaps": {
          "apiKey": "API-key"
        }
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "SEND_SMS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "INTERNET",
        "POST_NOTIFICATIONS",
        "com.google.android.gms.permission.AD_ID",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#d6d9d3"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#d6d9d3"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "permissions": [
              "android.permission.ACCESS_FINE_LOCATION",
              "android.permission.POST_NOTIFICATIONS",
              "android.permission.CAMERA"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "2e04fce3-d5b1-4292-ade0-021a4e6f8eb6"
      },
      "GOOGLE_MAPS_API_KEY": "@env.GOOGLE_MAPS_API_KEY",
      "EXPO_URL_API": "https://www.pinkker.tv/9000",
      "EXPO_URL_APIWS": "wss://www.pinkker.tv/9000",
      "router": {
        "origin": false
      }
    },
    "owner": "brunopereyra"
  }
}